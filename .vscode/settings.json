{
  "files.exclude": {
    "**dist": false,
    "**.webpack": false,
    "**node_modules": false,
    "**coverage": false
  },
  "rest-client.environmentVariables": {
    "$shared": {
      "base": "/api",
      "signIn": "/sign-in",
      "generatKeyPair": "/generate-key-pair",
      "encrypt": "/encrypt"
    },
    "local": {
      "host": "localhost:3000",
      "protocol": "http"
    }
  },
  "path-intellisense.mappings": {
    "~modules/*": "${workspaceRoot}/src/modules/*}",
    "~common": "${workspaceRoot}/src/common/index",
    "~config": "${workspaceRoot}/src/config/index",
    "~types": "${workspaceRoot}/src/types/index",
    "~decorators": "${workspaceRoot}/src/decorators/index",
    "~utils": "${workspaceRoot}/src/utils/index",
    "~repositories/*": "${workspaceRoot}/src/repositories/*"
  }
}
